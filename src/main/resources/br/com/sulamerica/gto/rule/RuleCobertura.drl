package br.com.sulamerica.gto.rule;

import br.com.sulamerica.gto.model.Procedimento;
import static br.com.sulamerica.odonto.gto.QueryRol;

rule "Procedimento consta no Rol"
    enabled true
    no-loop true
    dialect "mvel"
    
    when
        proc:Procedimento(eval(QueryRol.contains(codigoProcedimento) == true))
    then
        System.out.println("Consta no Rol")
end