package br.com.sulamerica.gto.rule;

import br.com.sulamerica.gto.model.Procedimento;

rule "Coloca procedimento em analise caso nao ative nenhuma regra"
    no-loop true
    dialect "mvel"
    agenda-group "regras_soberanas"
    enabled true
    when
        proc: Procedimento (eval( registrosAnaliseEletronica == null ) )
    then
        System.out.println("Executei a regra soberana ======== ÊÊÊÊÊÊÊÊÊ ============ Uhuuuuu ========");
        proc.setAnalisadoEletronicamente(true);
        proc.setStatusPagAposAnaiseEletronica("Análise");
        update(proc);
end
